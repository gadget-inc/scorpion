# kubeval-ignore
---
apiVersion: certmanager.k8s.io/v1alpha1
kind: Certificate
metadata:
  name: scorpion.gadget.dev-production
spec:
  secretName: scorpion.gadget.dev-production
  issuerRef:
    name: letsencrypt-production
    kind: ClusterIssuer
  dnsNames:
    - "scorpion.gadget.dev"
  acme:
    config:
      - dns01:
          provider: gke-clouddns
        domains:
          - "scorpion.gadget.dev"
---
apiVersion: certmanager.k8s.io/v1alpha1
kind: Certificate
metadata:
  name: scorpion-admin.gadget.dev-production
spec:
  secretName: scorpion-admin.gadget.dev-production
  issuerRef:
    name: letsencrypt-production
    kind: ClusterIssuer
  dnsNames:
    - "scorpion-admin.gadget.dev"
  acme:
    config:
      - dns01:
          provider: gke-clouddns
        domains:
          - "scorpion-admin.gadget.dev"
---
apiVersion: certmanager.k8s.io/v1alpha1
kind: Certificate
metadata:
  name: scorpion-assets.gadget.dev-production
spec:
  secretName: scorpion-assets.gadget.dev-production
  issuerRef:
    name: letsencrypt-production
    kind: ClusterIssuer
  dnsNames:
    - "scorpion-assets.gadget.dev"
  acme:
    config:
      - dns01:
          provider: gke-clouddns
        domains:
          - "scorpion-assets.gadget.dev"
