# kubeval-ignore
---
apiVersion: certmanager.k8s.io/v1alpha1
kind: Certificate
metadata:
  name: scorpion.superpro.io-production
spec:
  secretName: scorpion.superpro.io-production
  issuerRef:
    name: letsencrypt-production
    kind: ClusterIssuer
  dnsNames:
    - "scorpion.superpro.io"
  acme:
    config:
      - dns01:
          provider: gke-clouddns
        domains:
          - "scorpion.superpro.io"
---
apiVersion: certmanager.k8s.io/v1alpha1
